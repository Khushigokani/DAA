      "source": [
        "#0/1 knapsack using greedy approach\n",
        "values=[100,200,350]\n",
        "weight=[10,25,50]\n",
        "cap=50\n",
        "ratio=[v/w for v,w in zip(values,weight)]\n",
        "zipp=zip(ratio, values, weight)\n",
        "zipp=sorted(zipp,reverse=True)\n",
        "profit=0\n",
        "for i in zipp:\n",
        "  if(i[2]<=cap):\n",
        "    profit=profit+i[1]\n",
        "    cap=cap-i[2]\n",
        "\n",
        "print(profit)"
      ]
    }
  ]
}